clc 
clear 

syms x;
fx= input("INGRESA LA FUNCION :")
funcion = inline (char(fx))
a= input("COLOCA EL VALOR DEL LIMITE INFERIOR "); 
b= input("COLOCA EL VALOR DEL LIMITE SUPERIOR ");
resultado= (a*b)-((4*b)(sqrt(a*b)))/3 + b^2/2 - (a*a)-((4*a)(sqrt(a*a)))/3 + a^2/2

x = 1:0.2:0;
f=x;
plot(f,x);
bar(f,x,"r")
